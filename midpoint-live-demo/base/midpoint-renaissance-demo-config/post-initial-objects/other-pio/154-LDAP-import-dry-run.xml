                <q:equal>
                    <q:path>intent</q:path>
                    <q:value>default</q:value>
                </q:equal>
            </q:and>
<!--            <q:text>resourceRef matches ( targetRef = "ef2bc95b-76e0-48e2-86d6-3d4f02d3fafe" ) and kind = account and intent = default</q:text>-->
        </q:filter>
    </query>
</search>
