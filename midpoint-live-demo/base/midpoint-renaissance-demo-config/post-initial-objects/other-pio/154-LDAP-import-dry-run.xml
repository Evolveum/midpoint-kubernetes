        xmlns:q="http://prism.evolveum.com/xml/ns/public/query-3">
    <type>ShadowType</type>
    <query>
        <q:filter>
            <q:and>
                <q:ref>
                    <q:path>resourceRef</q:path>
                    <q:value oid ="ebd0bf7b-7e80-4175-ba5e-4fd5de2ecd62"/>
                </q:ref>
                <q:equal>
                    <q:path>kind</q:path>
                    <q:value>account</q:value>
