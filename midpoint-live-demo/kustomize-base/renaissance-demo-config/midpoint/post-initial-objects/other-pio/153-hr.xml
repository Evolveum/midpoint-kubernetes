                    <name>Corporate email address</name>
                    <lifecycleState>proposed</lifecycleState>
                    <strength>strong</strength>
                    <expression>
                        <script>
                            <code>basic.norm(input) + '@leonardo-workshop.org'</code>
                        </script>
                    </expression>
                    <target>
                        <path>emailAddress</path>
                    </target>
